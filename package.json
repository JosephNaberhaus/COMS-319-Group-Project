{
    "name": "coms319-project",
    "version": "1.0.0",
    "description": "CS 319 group project",
    "main": "server.js",
    "scripts": {
        "setup": "./scripts/dotenv.sh -f && npm i",
        "build": "tsc",
        "prestart": "npm run build",
        "start": "node -r esm src/server.js",
        "devstart": "node -r esm --require dotenv/config src/server.js",
        "dev": "nodemon",
        "test": "ls"
    },
    "repository": {
        "type": "git",
        "url": "git+https://git.linux.iastate.edu/319spring19projectrepositories/taghreed/group-14.git"
    },
    "keywords": [
        "CS319",
        "Sockets",
        "TypeScript",
        "NodeJS",
        "javascript"
    ],
    "author": "Maxwell DeVos, Mason Timmerman, John Jago, Joseph Naberhaus",
    "license": "ISC",
    "devDependencies": {
        "@types/dotenv": "^6.1.0",
        "@types/express": "^4.16.1",
        "@types/ip": "^1.1.0",
        "@types/node": "^11.10.4",
        "@types/socket.io": "^2.1.2",
        "@types/uuid": "^3.4.4",
        "typescript": "^3.3.4000"
    },
    "dependencies": {
        "dotenv": "^6.2.0",
        "esm": "^3.2.9",
        "express": "^4.16.4",
        "ip": "^1.1.5",
        "nodemon": "^1.18.10",
        "socket.io": "^2.2.0",
        "uuid": "^3.3.2",
        "vash": "^0.12.6"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "exec": "npm run build && npm run devstart",
        "ext": "ts"
    }
}
